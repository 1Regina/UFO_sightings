<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Shapes</title>
  </head>
  <body>
    <%- include('navigator_bar') -%>
     <div class="container"><h1>All UFO Shapes</h1>
        <%for (const [key, value] of Object.entries(shapeCounterObject)) {%>
        <ul> 
            <% let url = `/shapes/${key}` %>
            <h5>
              <a href="<%=url %>"><%=key%></a>
                <%=value%>
            </h5>
        </ul>
        <%}%>  
      </div> 
  </body>
</html>
